@model IEnumerable<NexxtDent.Models.Technical>

@{
    ViewBag.Title = @Resources.Resource.Technical_ViewIndex_Titulo1;
}

    <h2>@Resources.Resource.Technical_ViewIndex_Titulo1</h2>

<p>
    @Html.ActionLink(@Resources.Resource.ButtonNew, "Create", new { }, new { @class="btn btn-primary"})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WorkStation.Estacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City.Ciudad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Zone.Zona)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Date)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.FullName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.WorkStation.Estacion)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Phone)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.City.Ciudad)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Zone.Zona)
    </td>
    <td>
        @Html.ActionLink(@Resources.Resource.ViewEdit, "Edit", new { id = item.TechnicalId }, new { @class="btn btn-warning"}) |
        @Html.ActionLink(@Resources.Resource.ViewDetail, "Details", new { id = item.TechnicalId }, new { @class="btn btn-info"}) |
        @Html.ActionLink(@Resources.Resource.ViewDelete, "Delete", new { id = item.TechnicalId }, new { @class="btn btn-danger"})
    </td>
</tr>
}

</table>
