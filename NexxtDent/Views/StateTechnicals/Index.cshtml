@model IEnumerable<NexxtDent.Models.StateTechnical>

@{
    ViewBag.Title = @Resources.Resource.State_ViewIndex_Titulo;
}

    <h2>@Resources.Resource.State_ViewIndex_Titulo</h2>

<p>
    @Html.ActionLink(@Resources.Resource.ButtonNew, "Create", new { }, new { @class="btn btn-primary"})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EstadoTecnico)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EstadoTecnico)
        </td>
        <td>
            @Html.ActionLink(@Resources.Resource.ViewEdit, "Edit", new { id=item.StateTechnicalId }, new { @class="btn btn-warning"}) |
            @Html.ActionLink(@Resources.Resource.ViewDetail, "Details", new { id=item.StateTechnicalId }, new { @class="btn btn-info"}) |
            @Html.ActionLink(@Resources.Resource.ViewDelete, "Delete", new { id=item.StateTechnicalId }, new { @class="btn btn-danger"})
        </td>
    </tr>
}

</table>
